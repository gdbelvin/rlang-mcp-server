{
  "name": "R-Server",
  "version": "0.1.0",
  "description": "A Model Context Protocol that executes RMarkdown",
  "private": true,
  "type": "module",
  "bin": {
    "R-Server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:utils": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/__tests__/utils.test.ts src/__tests__/simple.test.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "dockerode": "^4.0.4"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.35",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.3.3"
  }
}
